{
  "plugins": [],
  "recurseDepth": 20,
  "source": {
    "exclude": [
      "node_modules",
      "docs",
      "CONCEPTS",
      "autocomplete",
      "marquis.screeps.ch/bins"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_",
    "include": [
      "marquis.screeps.ch/activities"
    ]
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "encoding": "utf8",
    "recurse": true
  }
}